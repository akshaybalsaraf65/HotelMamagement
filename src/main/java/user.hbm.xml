<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" > 
	
	<hibernate-mapping>
	<class name="beanpojo.user" table="user">
	<id name="id">
	<generator class="native"></generator>
	</id>
	<property name="fn" column="fname"></property>
	<property name="ln" column="lname"></property>
	<property name="mob" column="mobile"></property>
	<property name="usern" column="username"></property>
	<property name="pass" column="password"></property>
	<property name="cpass" column="cpassword"></property>
	<property name="active" column="active"></property>
	<property name="createdate" column="createddate"></property>
	</class>
	</hibernate-mapping>